<?xml version='1.0' encoding='UTF-8'?>
<!-- Created with Cambalache 0.96.1 -->
<interface>
  <!-- interface-name test.ui -->
  <requires lib="gtk" version="4.0"/>
  <requires lib="libadwaita" version="1.0"/>
  <object class="AdwApplicationWindow" id="main_window">
    <property name="default-height">600</property>
    <property name="default-width">800</property>
    <property name="title" translatable="yes" context="title">My program</property>
    <child>
      <object class="GtkBox">
        <property name="orientation">vertical</property>
        <child>
          <object class="AdwHeaderBar" id="header_bar">
            <property name="title-widget">
              <object class="GtkLabel">
                <property name="label" translatable="yes" context="title">My program</property>
              </object>
            </property>
          </object>
        </child>
        <child>
          <object class="GtkScrolledWindow">
            <property name="child">
              <object class="GtkBox" id="main_box">
                <property name="margin-bottom">12</property>
                <property name="margin-end">12</property>
                <property name="margin-start">12</property>
                <property name="margin-top">12</property>
                <property name="orientation">vertical</property>
                <property name="spacing">12</property>
                <property name="vexpand">True</property>
                <child>
                  <object class="GtkScrolledWindow" id="scrolled_window_dates">
                    <property name="child">
                      <object class="GtkBox" id="date_stack">
                        <property name="spacing">8</property>
                      </object>
                    </property>
                    <property name="height-request">60</property>
                    <property name="hexpand">True</property>
                    <property name="vscrollbar-policy">never</property>
                  </object>
                </child>
                <child>
                  <object class="GtkLabel" id="battery_chart_label">
                    <property name="label" translatable="yes" context="battery_chart">Battery Chart</property>
                    <property name="margin-top">12</property>
                    <property name="xalign">0.0</property>
                  </object>
                </child>
                <child>
                  <object class="GtkBox" id="chart_container">
                    <property name="hexpand">True</property>
                    <property name="vexpand">False</property>
                    <child>
                      <object class="GtkBox" id="battery_chart_placeholder"/>
                    </child>
                  </object>
                </child>
              </object>
            </property>
            <property name="vexpand">True</property>
            <property name="vscrollbar-policy">automatic</property>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
